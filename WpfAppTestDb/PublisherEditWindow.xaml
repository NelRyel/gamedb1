<Window x:Class="WpfAppTestDb.PublisherEditWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppTestDb"
        mc:Ignorable="d"
        Title="PublisherEditWindow" Height="300" Width="450"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <StackPanel Orientation="Vertical" Margin="5">
            <Label x:Name="labelOldName" Margin="5" Height="30" HorizontalAlignment="Center" VerticalAlignment="Center" ></Label>
            <TextBox x:Name="tbEditPub" Margin="5" Height="30"></TextBox>
            <Button Margin="5" Height="30" Click="Button_Click_Ok">OK</Button>
            <Button Margin="5" Height="30" Click="Button_Click_Cancel">Cancel</Button>
        </StackPanel>
    </Grid>
</Window>
