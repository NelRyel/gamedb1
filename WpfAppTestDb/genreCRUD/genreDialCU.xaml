<Window x:Class="WpfAppTestDb.genreCRUD.genreDialCU"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppTestDb.genreCRUD"
        mc:Ignorable="d"
        Title="genreDialCU" Height="300" Width="300"
         WindowStartupLocation="CenterScreen">
    <Grid>
        <StackPanel Orientation="Vertical" Margin="5">
            <Label x:Name="labelWinTitle"></Label>
            <Label x:Name="labelOldName" Margin="5" Height="30" HorizontalAlignment="Center" VerticalAlignment="Center" ></Label>
            <TextBox x:Name="tbEditName" Margin="5" Height="30"></TextBox>
            <Button x:Name="btnOk" Margin="5" Height="30">OK</Button>
            <Button x:Name="btnClose" Margin="5" Height="30" Click="btnClose_Click"  >Cancel</Button>
        </StackPanel>
    </Grid>
</Window>
